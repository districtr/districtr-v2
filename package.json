{
  "name": "districtr-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write \"{app,utils}/**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "latest",
    "prettier": "^3.0.3",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "typescript": "latest"
  },
  "lint-staged": {
    "{app,utils}/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": "eslint --cache --fix"
  }
}
